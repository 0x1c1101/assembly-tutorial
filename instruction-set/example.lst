     1                                  ;; NOTE that this is not a program meant to be run.  It is just a way to demonstrate
     2                                  ;; that the instructions and addressing modes to assemble without error.
     3                                  
     4                                  global start
     5                                  start:
     6                                          ;; register addressing mode
     7 00000000 4889D8                          mov rax, rbx
     8                                  
     9                                          ; direct (or immediate) addressing mode
    10                                          ; you cannot store to a constant, so only the source may be a constant
    11 00000003 B80A000000                      mov rax, 10 	; source operand is a constant
    12                                  
    13                                          ; indirect addressing mode
    14                                          ; one of the operands is the address of the memory location in a register
    15 00000008 488B03                          mov rax, [rbx]
    16 0000000B 488903                          mov [rbx], rax
    17                                          ; invalid!
    18                                          ; mov [rax], [rbx]
    19                                  
    20                                          ; indirect with displacement
    21                                          ; address = base + displacement
    22 0000000E 488B4318                        mov rax, [24+rbx] 	; base is rbx, displacement is 24
    23 00000012 48894318                        mov [24+rbx], rax
    24                                  
    25                                          ; indirect with displacement and scaled index
    26 00000016 488B049D[00000000]              mov rax, [array + rbx * 4]
    27                                          
    28                                         section .bss 
    29 00000000 <res 00002000>          array: resb 8192
